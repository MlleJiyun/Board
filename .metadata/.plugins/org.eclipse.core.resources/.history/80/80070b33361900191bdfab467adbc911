<mapper namespace="org.jiyunkim1.mapper.postMapper">

<select id="getTime" resultType="string">
select now()
</select>

<insert id="insertPost">
insert into tbl_post (postId, postTitle, userId,postContent) values
(#{postId}, #{postTitle}, #{userId}, #{postContent})
</insert>

</mapper>