package org.jiyunkim1.domain;

public class PostVO {
	
	private String name;
	private double price;
	
	public PostVO (String name, double price) {
		super();
		this.name=name;
		this.price=price;
	}
	public String getName() {
		return name;
	}
	
	public double getPrice() {
		return price;
	}
	
	@Override
	public String toString() {
		return "ProductVO [name=" + name + ",price=" +  price + "]";
	}

}
